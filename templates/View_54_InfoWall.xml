	<include name="View_54_InfoWall">
		<control type="group">
			<include>OpenClose_Right</include>
			<left>{{ScaleX('100')}}</left>
			<top>{{ScaleY('0')}}</top>
			<visible>Control.IsVisible(54)</visible>
			<include>Visible_Right</include>
			<control type="panel" id="54">
				<left>{{ScaleX('490')}}</left>
				<top>{{ScaleY('0')}}</top>
				<right>{{ScaleX('15')}}</right>
				<height>{{ScaleY('100%')}}</height>
				<onleft>9000</onleft>
				<onright>531</onright>
				<onup condition="Integer.IsGreater(Container.NumPages,1)">54</onup>
				<onup condition="!Integer.IsGreater(Container.NumPages,1)">9000</onup>
				<ondown>54</ondown>
				<viewtype label="31101">icon</viewtype>
				<preloaditems>2</preloaditems>
				<pagecontrol>531</pagecontrol>
				<scrolltime tween="cubic" easing="out">500</scrolltime>
				<visible>Container.Content(artists) | Container.Content(albums) | Container.Content(sets) | Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(musicvideos) | Container.Content(images) | Container.Content(videos)</visible>
				<itemlayout height="{{ScaleY('445')}}" width="{{ScaleX('{{ 320 + {InfoWallExtraWidth} }}')}}" condition="Container.Content(movies) | Container.Content(sets) | Container.Content(tvshows) | Container.Content(seasons)">
					<control type="group">
						<left>{{ScaleX('30')}}</left>
						<top>{{ScaleY('120')}}</top>
						<include>InfoWallMovieLayout</include>
					</control>
				</itemlayout>
				<focusedlayout height="{{ScaleY('445')}}" width="{{ScaleX('{{ 320 + {InfoWallExtraWidth} }}')}}" condition="Container.Content(movies) | Container.Content(sets) | Container.Content(tvshows) | Container.Content(seasons)">
					<control type="group">
						<depth>DepthContentPopout</depth>
						<left>{{ScaleX('30')}}</left>
						<animation effect="zoom" start="100" end="110" time="200" tween="sine" easing="inout" center="{{ScaleBorder2('{{ 190 + {InfoWallExtraWidth}/2 }},300')}}">Focus</animation>
						<animation effect="zoom" start="110" end="100" time="200" tween="sine" easing="inout" center="{{ScaleBorder2('{{ 190 + {InfoWallExtraWidth}/2 }},300')}}">UnFocus</animation>
						<top>{{ScaleY('120')}}</top>
						<include content="InfoWallMovieLayout">
							<param name="focused" value="true" />
						</include>
					</control>
				</focusedlayout>
				<itemlayout height="{{ScaleY('300')}}" width="{{ScaleX('{{ 300 + {InfoWallExtraWidth} }}')}}" condition="Container.Content(episodes) | Container.Content(videos) | Container.Content(musicvideos)">
					<control type="group">
						<left>{{ScaleX('64')}}</left>
						<top>{{ScaleY('110')}}</top>
						<include content="InfoWallEpisodeLayout">
							<param name="focused" value="false" />
							<param name="thumb_label" value="$INFO[ListItem.Label2]" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout height="{{ScaleY('300')}}" width="{{ScaleX('{{ 300 + {InfoWallExtraWidth} }}')}}" condition="Container.Content(episodes) | Container.Content(videos) | Container.Content(musicvideos)">
					<control type="group">
						<depth>DepthContentPopout</depth>
						<animation effect="zoom" start="100" end="110" time="200" tween="sine" easing="inout" center="{{ScaleBorder2('{{ 220 + {InfoWallExtraWidth}/2 }},250')}}">Focus</animation>
						<animation effect="zoom" start="110" end="100" time="200" tween="sine" easing="inout" center="{{ScaleBorder2('{{ 220 + {InfoWallExtraWidth}/2 }},250')}}">UnFocus</animation>
						<left>{{ScaleX('64')}}</left>
						<top>{{ScaleY('110')}}</top>
						<include content="InfoWallEpisodeLayout">
							<param name="focused" value="true" />
							<param name="thumb_label" value="$INFO[ListItem.Label2]" />
						</include>
					</control>
				</focusedlayout>
				<itemlayout height="{{ScaleY('410')}}" width="{{ScaleX('{{ 310 + {InfoWallExtraWidth} }}')}}" condition="Container.Content(artists) | Container.Content(albums)">
					<control type="group">
						<top>{{ScaleY('150')}}</top>
						<left>{{ScaleX('40')}}</left>
						<include>InfoWallMusicLayout</include>
					</control>
				</itemlayout>
				<focusedlayout height="{{ScaleY('410')}}" width="{{ 310 + {InfoWallExtraWidth} }}" condition="Container.Content(artists) | Container.Content(albums)">
					<control type="group">
						<depth>DepthContentPopout</depth>
